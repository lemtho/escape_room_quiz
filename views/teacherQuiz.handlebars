        <!-- header-start -->
        <div style="height:100px;">
            <header>
                <div class="header-area" style="background-color:rgb(89,0,179)">
                    <div class="main-header-area">
                        <div class="container-fluid p-0">
                            <div class="row align-items-center no-gutters" style="height:100px;">
                                <div class="col-xl-2 col-lg-2">
                                    <div class="logo-img">
                                        <a href="/teacherHomePage">
                                            <img src="img/logo.png" alt="">
                                        </a>
                                    </div>
                                </div>
                                <div class="col-xl-7 col-lg-7">
                                    <div class="main-menu  d-none d-lg-block">
                                        <nav>
                                            <ul id="navigation">
                                                <li><a href="/teacherHomePage">Home</a></li>
                                                <li><a class="active" href="/teacherQuiz">My Quizzes</a></li>
                                                <li><a href="/teacherScoreboard">Scoreboard</a></li>
                                                <li><a href="/teacherProfile">My Profile</a></li>
                                            </ul>
                                        </nav>
                                    </div>
                                </div>
                                <div class="col-xl-3 col-lg-3 d-none d-lg-block">
                                    <div>
                                        <a href="/logout">
                                            <i style="color:white;" class="flaticon-user"></i>
                                            <span style="color:white;">Log Out</span>
                                        </a>
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="mobile_menu d-block d-lg-none"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
        </div>
        <!-- header-end -->

        <!-- content_area_start -->
        <div class="slider_area ">
            <div class="single_slider slider_bg_1" style="min-height:75vh; height:max-content; padding-top:5%; padding-bottom:5%;">
                <div class="container" style="text-align:center;">

                    
                    <div>
                        <!--<h1 style="color:white;">PUT STUFF HERE</h1>-->
                        <table class = "spacing">
                            <thead>
                                <th colspan="2" align="left"><h2>Quiz Code</h2></th>
                                <th colspan="4" align = "left"><h2>Quiz Name</h2></th>
                                <th colspan="4"><h2>Number of Questions</h2></th>
                                <th colspan="4"><h2>Number Taken</h2></th>
                            </thead>
                            <tbody>
                                {{#if quiz}}
                                {{#each quiz}}
                                        <tr>
                                            <td colspan="2">{{id}}</td>
                                            <td colspan="4">{{name}}</td>
                                            <td colspan="4">{{numQuestion}}</td>
                                            <td class = "cannot_change" colspan="4">{{num_taken}}</td>
                                            {{#showResult num_taken}}
                                            <td><button onclick="fetchResults({{id}},'{{name}}')" class = "results">Results</button></td>
                                            {{/showResult}}
                                            {{#editdel num_taken}}
                                            <td><button onclick = "window.location.href = '/teacherQuiz/{{id}}';" class = "edit">Edit</button></td>
                                            <td><button onclick = "deleteQuiz({{id}})" class = "delete">Delete</button></td>
                                            {{/editdel}}
                                        </tr>
                                {{/each}}
                                {{else}}
                                    <tr><td colspan = "10"><h4><span style ="font-weight: normal; text-align: center">You don't have any quiz. Click on "Create New Quiz" to get started.</span></h4></td></tr>
                                {{/if}}
                                    <tr><td colspan = "8" align = "right"><button onclick = "window.location.href = '/teacherQuiz/createQuiz';" class = "create">Create New Quiz</button></td></tr>
                            </tbody>
                        </table>
                    </div>
                
                </div>
            </div>
        </div>
        <!-- content_area_end -->

        <!-- footer -->
        <footer class="footer footer_bg_1">
