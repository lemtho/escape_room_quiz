        <!-- header-start -->
        <div style="height:100px;">
            <header>
                <div class="header-area" style="background-color:rgb(89,0,179)">
                    <div class="main-header-area">
                        <div class="container-fluid p-0">
                            <div class="row align-items-center no-gutters" style="height:100px;">
                                <div class="col-xl-2 col-lg-2">
                                    <div class="logo-img">
                                        <a href="/teacherHomePage">
                                            <img src="img/Our Logo.png" alt="">
                                        </a>
                                    </div>
                                </div>
                                <div class="col-xl-7 col-lg-7">
                                    <div class="main-menu  d-none d-lg-block">
                                        <nav>
                                            <ul id="navigation">
                                                <li><a href="/teacherHomePage">Home</a></li>
                                                <li><a class="active" href="/teacherQuiz">My Quizzes</a></li>
                                                <li><a href="/teacherScoreboard">Scoreboard</a></li>
                                                <li><a href="/teacherProfile">My Profile</a></li>
                                            </ul>
                                        </nav>
                                    </div>
                                </div>
                                <div class="col-xl-3 col-lg-3 d-none d-lg-block">
                                    <div>
                                        <a href="/logout">
                                            <i style="color:white;" class="flaticon-user"></i>
                                            <span style="color:white;">Log Out</span>
                                        </a>
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="mobile_menu d-block d-lg-none"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
        </div>
        <!-- header-end -->

        <!-- content_area_start -->
        <div class="slider_area" style="min-height:100vh;">
            <div class="single_slider slider_bg_1" style="min-height:100vh; height:max-content; padding-top:5%; padding-bottom:5%;">
                <div class="container" style="text-align:center;">
                    <div> 
                    {{#if quiz}}
                        <table class = "table table-dark" style="background-color:rgba(90, 0, 179, 0.5); color:black;">
                            <thead>
                                <th style="border:unset;" colspan="2" align="left"><h3>Quiz Code</h3></th>
                                <th style="border:unset;" colspan="4" align = "left"><h3>Quiz Name</h3></th>
                                <th style="border:unset;" colspan="4"><h3>Number of Questions</h3></th>
                                <th style="border:unset;" colspan="4"><h3>Number Taken</h3></th>
                                <th style="border:unset;" colspan="4"><h3>Published</h3></th>
                                <th style="border:unset;" colspan="3"><h3>Action</h3></th>
                            </thead>
                            <tbody>
                                {{#each quiz}}
                                        <tr>
                                            <td style="border:unset;" colspan="2">{{calcCode published id}}</td>
                                            <td style="border:unset;" colspan="4">{{name}}</td>
                                            <td style="border:unset;" colspan="4">{{numQuestion}}</td>
                                            <td style="border:unset;" colspan="4">{{num_taken}}</td>
                                            <td style="border:unset;" colspan="4">{{published}}</td>
                                            {{#showResult published}}
                                            <td style="border:unset;"><button class="btn btn-primary btn-sm" onclick="fetchResults({{id}},'{{name}}')">Results</button></td>
                                            {{/showResult}}
                                            {{#editdel published}}
                                            <td style="border:unset;"><button class="btn btn-primary btn-sm" style="background-color:rgba(45,226,84, 0.75);" onclick = "">Publish</button></td>
                                            <td style="border:unset;"><button class="btn btn-primary btn-sm" onclick = "window.location.href = '/teacherQuiz/{{id}}';">Edit</button></td>
                                            <td style="border:unset;"><button class="btn btn-primary btn-sm" style="background-color: rgba(255, 0, 0, 0.75);" onclick = "deleteQuiz({{id}})">Delete</button></td>
                                            {{/editdel}}
                                        </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    {{else}}
                        <h4><span style ="font-weight: normal; text-align: center">You don't have any quiz. Click on "Create New Quiz" to get started.</span></h4>
                    {{/if}}
                        <br>
                        <button onclick = "window.location.href = '/teacherQuiz/createQuiz';" class="btn btn-primary btn-sm" style="font-weight: bold;">Create New Quiz</button>
                    </div>
                </div>
            </div>
        </div>
        <!-- content_area_end -->

        <!-- footer -->
        <footer class="footer footer_bg_1">
            <div class="copy-right_text" style="background-color:rgb(89,0,179)">
