<!doctype html>
<html class="no-js" lang="zxx">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>{{title}}</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- <link rel="manifest" href="site.webmanifest"> -->
    <link rel="shortcut icon" type="image/x-icon" href="img/favicon.png">
    <!-- Place favicon.ico in the root directory -->

    <!-- CSS here -->
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" href="css/owl.carousel.min.css">
    <link rel="stylesheet" href="css/magnific-popup.css">
    <link rel="stylesheet" href="css/font-awesome.min.css">
    <link rel="stylesheet" href="css/themify-icons.css">
    <link rel="stylesheet" href="css/nice-select.css">
    <link rel="stylesheet" href="css/flaticon.css">
    <link rel="stylesheet" href="css/gijgo.css">
    <link rel="stylesheet" href="css/animate.css">
    <link rel="stylesheet" href="css/slicknav.css">
    <link rel="stylesheet" href="css/style.css">
    <!-- <link rel="stylesheet" href="css/responsive.css"> -->
</head>

<body style="font-family:-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;">

    <!-- header-start -->
    <div style="height:100px;">
        <header>
            <div class="header-area" style="background-color:rgb(89,0,179)">
                <div class="main-header-area">
                    <div class="container-fluid p-0">
                        <div class="row align-items-center no-gutters" style="height:100px;">
                            <div class="col-xl-2 col-lg-2">
                                <div class="logo-img">
                                    <a href="/teacherHomePage">
                                        <img src="img/logo.png" alt="">
                                    </a>
                                </div>
                            </div>
                            <div class="col-xl-7 col-lg-7">
                                <div class="main-menu  d-none d-lg-block">
                                    <nav>
                                        <ul id="navigation">
                                            <li><a class="active" href="/teacherHomePage">Home</a></li>
                                            <li><a href="/teacherQuiz">My Quizzes</a></li>
                                            <li><a href="/teacherStudents">My Students</a></li>
                                            <li><a href="/teacherScoreboard">Scoreboard</a></li>
                                            <li><a href="/teacherProfile">My Profile</a></li>
                                        </ul>
                                    </nav>
                                </div>
                            </div>
                            <div class="col-xl-3 col-lg-3 d-none d-lg-block">
                                <div>
                                    <a href="/logout">
                                        <i style="color:white;" class="flaticon-user"></i>
                                        <span style="color:white;">Log Out</span>
                                    </a>
                                </div>
                            </div>
                            <div class="col-12">
                                <div class="mobile_menu d-block d-lg-none"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
    </div>
    <!-- header-end -->

    <!-- content_area_start -->
    <div class="slider_area ">
        <div class="single_slider slider_bg_1" style="min-height:75vh; height:max-content; padding-top:5%; padding-bottom:5%;">
            <div class="container" style="text-align:center;">

                <div>
                    <h1 style="color:white;">Welcome Back {{user}}!</h1>
                </div>
                <br>

                <div id="recentQuizzes">
                    <h3>Recently Taken Quizzes</h3>

                    <div>
                        <table class="spacing" align="center">
                            <thead>
                                <th colspan="4" align="left">First Name</th>
                                <th colspan="4">Last Name</th>
                                <th colspan="4">Quiz Name</th>
                                <th colspan="4">Date Taken</th>
                            </thead>
                            <tbody>

                                {{#each students}}
                                <tr>
                                    <td colspan="4">{{firstName}}</td>
                                    <td colspan="4">{{lastName}}</td>
                                    <td colspan="4">{{name}}</td>
                                    <td colspan="4">{{lastTakenDate}}</td>
                                    <td><button onclick="fetchResults({{studentID}})" class = "results">Scoreboard</button></td> 
                                </tr>
                                {{/each}}

                            </tbody>
                        </table>
                    </div>

                </div>
            
            </div>
        </div>
    </div>
    <!-- content_area_end -->

    <!-- footer -->
    <footer class="footer footer_bg_1">
        <div class="copy-right_text" style="background-color:rgb(89,0,179)">
            <div class="container">
                <div class="footer_border"></div>
                <div class="row">
                    <div class="col-xl-12">
                        <p class="copy_right text-center" style="font-family:-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;">
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                            Copyright &copy;<script>document.write(new Date().getFullYear());</script> All rights reserved | This template is made with <i class="fa fa-heart-o" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib</a>
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- footer -->

    <!-- JS here -->
    <script src="js/vendor/modernizr-3.5.0.min.js"></script>
    <script src="js/vendor/jquery-1.12.4.min.js"></script>
    <script src="js/popper.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/owl.carousel.min.js"></script>
    <script src="js/isotope.pkgd.min.js"></script>
    <script src="js/ajax-form.js"></script>
    <script src="js/waypoints.min.js"></script>
    <script src="js/jquery.counterup.min.js"></script>
    <script src="js/imagesloaded.pkgd.min.js"></script>
    <script src="js/scrollIt.js"></script>
    <script src="js/jquery.scrollUp.min.js"></script>
    <script src="js/wow.min.js"></script>
    <script src="js/nice-select.min.js"></script>
    <script src="js/jquery.slicknav.min.js"></script>
    <script src="js/jquery.magnific-popup.min.js"></script>
    <script src="js/plugins.js"></script>
    <script src="js/gijgo.min.js"></script>

    <script src="frontendJS/teacherHomePage.js"></script>

    <script src="js/main.js"></script>
</body>

</html>