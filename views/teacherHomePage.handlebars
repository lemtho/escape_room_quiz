        <!-- header-start -->
        <div style="height:100px;">
            <header>
                <div class="header-area" style="background-color:rgb(89,0,179)">
                    <div class="main-header-area">
                        <div class="container-fluid p-0">
                            <div class="row align-items-center no-gutters" style="height:100px;">
                                <div class="col-xl-2 col-lg-2">
                                    <div class="logo-img">
                                        <a href="/teacherHomePage">
                                            <img src="img/Our Logo.png" alt="">
                                        </a>
                                    </div>
                                </div>
                                <div class="col-xl-7 col-lg-7">
                                    <div class="main-menu  d-none d-lg-block">
                                        <nav>
                                            <ul id="navigation">
                                                <li><a class="active" href="/teacherHomePage">Home</a></li>
                                                <li><a href="/teacherQuiz">My Quizzes</a></li>
                                                <li><a href="/teacherScoreboard">Scoreboard</a></li>
                                                <li><a href="/teacherProfile">My Profile</a></li>
                                            </ul>
                                        </nav>
                                    </div>
                                </div>
                                <div class="col-xl-3 col-lg-3 d-none d-lg-block">
                                    <div>
                                        <a href="/logout">
                                            <i style="color:white;" class="flaticon-user"></i>
                                            <span style="color:white;">Log Out</span>
                                        </a>
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="mobile_menu d-block d-lg-none"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
        </div>
        <!-- header-end -->

        <!-- content_area_start -->
        <div class="slider_area" style="min-height:100vh;">
            <div class="single_slider slider_bg_1" style="min-height:100vh; height:max-content; padding-top:5%; padding-bottom:5%;">
                <div class="container" style="text-align:center;">

                    <div>
                        <h1 style="color:white;">Welcome Back {{user}}!</h1>
                    </div>
                    <br>

                    <div id="recentQuizzes">
                            <table id="activityTable" class="spacing">
                                <thead>
                                    <tr>
                                        <th style="visibility: collapse;">date</th>
                                        <th style="visibility: collapse;">activity</th>
                                        <th style="visibility: collapse;">link</th>
                                    </tr>
                                </thead>

                                <tbody>
                                    {{#each students}}
                                    <tr>
                                            <td>{{lastTakenDate}}</td>
                                            <td><b>{{firstName}} {{lastName}}</b> completed <b>{{name}}</b> quiz!</td>
                                            <td><button onclick="fetchResults({{studentID}})" class = "results">Scoreboard</button></td>
                                    </tr>
                                    {{/each}}
                                    {{#each quizzes}}
                                    <tr>
                                            <td>{{dateCreated}}</td>
                                            <td><b>{{quizName}}</b> quiz created!</td>
                                    </tr>
                                    {{/each}}
                                </tbody>
                            </table>
                    </div>


                    <div id="sortedActivities" style="text-align: center;">
                        <h2>Recent Activity</h2>
                        <table id="sortedTable" class="spacing" style="text-align: left;" align="center">
                            
                        </table>
                    </div>

                </div>
            </div>
        </div>
        <!-- content_area_end -->

        <!-- footer -->
