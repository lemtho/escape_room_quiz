        <!-- header-start -->
        <div style="height:100px;">
            <header>
                <div class="header-area" style="background-color:rgb(89,0,179)">
                    <div class="main-header-area">
                        <div class="container-fluid p-0">
                            <div class="row align-items-center no-gutters" style="height:100px;">
                                <div class="col-xl-2 col-lg-2">
                                    <div class="logo-img">
                                        <a href="/studentHomePage">
                                            <img src="/img/Our Logo.png" alt="">
                                        </a>
                                    </div>
                                </div>
                                <div class="col-xl-7 col-lg-7">
                                    <div class="main-menu  d-none d-lg-block">
                                        <nav>
                                            <ul id="navigation">
                                                <li><a href="/studentHomePage">Home</a></li>
                                                <li><a class="active" href="/studentScoreboard">My Scores</a></li>
                                                <li><a href="/studentProfile">My Profile</a></li>
                                            </ul>
                                        </nav>
                                    </div>
                                </div>
                                <div class="col-xl-3 col-lg-3 d-none d-lg-block">
                                    <div>
                                        <a href="/logout">
                                            <i style="color:white;" class="flaticon-user"></i>
                                            <span style="color:white;">Log Out</span>
                                        </a>
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="mobile_menu d-block d-lg-none"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
        </div>
        <!-- header-end -->

        <!-- content_area_start -->
        <div class="slider_area" style="min-height:100vh;">
            <div class="single_slider slider_bg_1" style="min-height:100vh; height:max-content; padding-top:5%; padding-bottom:5%;">
                <div class="container" style="text-align:center;">

                    <div>
                        {{#each header}}
                        <h1 style="color:white;">Quiz: {{name}}</h1>
                        <h1 style="color:white;">Student: {{firstName}} {{lastName}}</h1>
                        <h3 style="color:white;" >Date Taken: {{lastTakenDate}}</h3>
                        {{/each}}
                    </div>

                    <div id="displayResults">
                        <table class="spacing" align="center">
                            <thead>
                                <tr>
                                    <th>Question #</th>
                                    <th>Question</th>
                                    <th>Student Answer</th>
                                    <th>Correct Answer</th>
                                    <th>Question Point</th>
                                </tr>
                            </thead>

                            <tbody>
                                {{#each questionResult}}
                                    <tr>
                                        <td>{{questionOrder}}</td>
                                        <td>{{question}}</td>
                                        <td>{{studentAnswer}}</td>
                                        <td>{{answer}}</td>
                                        <td>{{questionPT}}</td>
                                    </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div>
                    
                    <div id="displayFinalScore">
                        {{#each finalScore}}
                        <h1 style="color:white;">Final Score: {{actual_score}}/{{count}}</h1>
                        {{/each}}
                    </div>

                    <div id="printResult">
                        <form>
                            <input id="printPageButton" type="button" id="printResult" value="Save My Results" onclick="window.print()">
                        </form>
                    </div>

                </div>
            </div>
        </div>
        <!-- content_area_end -->

        <!-- footer -->
        <footer class="footer footer_bg_1">
            <div class="copy-right_text" style="background-color:rgb(89,0,179)">
                <div class="container">
                    <div class="footer_border"></div>
                    <div class="row">
                        <div class="col-xl-12">
                            <p class="copy_right text-center" style="font-family:-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;">
